\{{#each this}}
  <div class="seperator">\{{ @key }}</div>
  \{{#each this}}
  <div class="contact-card" data-contact-id="\{{ id }}">
    <div class="profile-image-holder \{{#if isChannel}}channel\{{/if}}">
      \{{#if image}}
        <div class="profile-image" style="background-image: url('\{{auth image }}')"></div>
      \{{/if}}
      \{{#if isChannel}}
      <i class="fa fa-users"></i>
      \{{else}}
      <i class="fa fa-user"></i>
      \{{/if}}
    </div>
    <div class="contact-info">
      <div class="contact-user-name">\{{ fullName }}</div>
      <div class="contact-user-preview-holder">
        <div class="contact-user-preview">\{{{ title }}}</div>
      </div>
    </div>
    \{{#if pinned}}
    <div class="icon icon-static icon-rotate-45"><i class="fa fa-thumb-tack"></i></div>
    \{{/if}}
  </div>
  \{{/each}}
\{{/each}}